<% content_for :title do %>
    <% "OpenSplitTime: List organizations" %>
<% end %>

<header class="ost-header">
  <div class="container">
    <div class="ost-heading row">
      <div class="col">
        <div class="ost-title">
          <h1>Organizations</h1>
          <ul class="breadcrumb">
            <li class="breadcrumb-item active"><%= link_to 'Organizations', organizations_path %></li>
          </ul>
        </div>
      </div>
    </div>
    <!-- Navigation -->
  </div>
</header>

<article class="ost-article container">
  <%= will_paginate(inner_window: 2, outer_window: 0) %>
  <%= render 'organizations', presenter: @presenter %>
  <%= will_paginate(inner_window: 2, outer_window: 0) %>
</article>
